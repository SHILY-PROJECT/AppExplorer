@inherits ListExplorerObjectsBase

@if (ListExplorerObjects != null)
{
    @foreach (var obj in ListExplorerObjects)
    {
        <DisplayExplorerObject ExplorerObject="obj" @ref="ChildDisplayExplorerObject" OnGoToRoute="GoToRoute" OnGoToRouteFromHistory="DisplayExplorerObjects"/>
    }
}
else
{
    <div class="spinner"></div>
}